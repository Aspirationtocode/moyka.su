include ../header-nav/header-nav
include ../header-cars/header-cars
include ../header-footer/header-footer
mixin header(activePage, menuElements)
	header.header
		+header-cars()
		.header__appear
		+header-nav(activePage, menuElements)
		+header-footer()
		.header-location-wrapper
			a(href="./index.html").header-logo
			.header-location
				.header-location__text
					| Мы находимся по адресу:
					br
					| г.Москва г.Троицк Калужское шоссе д. 38
				button.header-location__button Показать на карте
				.header-location__map
					script(type='text/javascript', charset='utf-8', async='', src='https://api-maps.yandex.ru/services/constructor/1.0/js/?um=constructor%3A28af424dd343bf904005ada8659098d357c84475aceb81967c05bfb680d80dbb&width=100%&height=100%&lang=ru_RU&scroll=true')
